+#include <sys/fcntl.h>
+
+/* Minimal fcntl support is necessary for C++ console streams cin, cout, and
+   cerr. */
+int
+_fcntl (int fd, int flag, ...)
+{
+  switch (flag) {
+     case F_GETFL:
+        switch (fd) {
+           case 0: return O_RDONLY;
+           case 1: return O_WRONLY;
+           case 2: return O_WRONLY;
+           default: return -1;
+        }
+     default:
+        return -1;
+  }
+
+  return -1;
+}
